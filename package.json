{
  "name": "leo-portfolio-tools",
  "version": "1.0.0",
  "description": "Portfolio optimization tools",
  "scripts": {
    "fetch:assets": "node scripts/fetch-assets.mjs",
    "optimize": "node optimize-media.js",
    "optimize:images": "node optimize-media.js --images-only",
    "optimize:videos": "node optimize-media.js --videos-only",
    "generate-projects": "node generate-projects.js",
    "copy:fonts": "node scripts/copy-fonts.mjs",
    "build": "npm run generate-projects && npm run fetch:assets && npm run copy:fonts && npm run optimize && node scripts/rev.mjs",
    "dev": "browser-sync start --server --no-ui --no-notify --host 127.0.0.1 --port 5530 --files \"index.html,*.css,main.js,js/**/*.js,projects/**/*.html,assets/**/*,manifest.webmanifest,sw.js,projects.json\"",
    "lh": "lhci autorun --collect.url=https://leok974.github.io/leo-portfolio/ --upload.target=temporary-public-storage",
    "snapshot": "node scripts/snapshot.mjs"
  },
  "dependencies": {
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "@fontsource-variable/inter": "^5.2.8",
    "@lhci/cli": "^0.13.0",
    "commander": "^11.0.0",
    "glob": "^10.3.0",
    "lighthouse": "^12.0.0",
    "tslib": "2.8.1"
  }
}
