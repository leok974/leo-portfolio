# Development environment variables
# Allow dev admin override via ?admin=1
VITE_ALLOW_DEV_ADMIN=1

# Agent API configuration for portfolio assistant
# Empty = same-origin (goes through nginx proxy on :8090, no CORS)
VITE_AGENT_API_BASE=

# Disable dynamic layout fetch in dev (avoids 404)
VITE_LAYOUT_ENABLED=0

# Calendly booking URL
VITE_CALENDLY_URL=https://calendly.com/leoklemet-pa
