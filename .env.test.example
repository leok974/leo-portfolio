# PLAYWRIGHT_GLOBAL_SETUP_SKIP=1
# BACKEND_REQUIRED=0
# WAIT_PRIMARY_MS=60000
# APP_ENV=dev
# OLLAMA_HOST=http://localhost:11434
# DEV_E2E_EMAIL=your.email@example.com
# DEV_E2E_PASSWORD=your-password
# DEV_SUPERUSER_PIN=123456
